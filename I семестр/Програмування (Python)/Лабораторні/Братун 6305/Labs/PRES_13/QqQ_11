>>> arr = [7, 2, 10, 4, 8, 6, 9, 3, 1, 5]
>>> arr.sort(reverse = True)
>>> arr                                             # Сортування за спаданням

>>> arr = ["sky","land","water","fire","sun"]
>>> arr.sort(reverse = True)
>>> arr

>>> arr = ["sky","land","water","fire",         "Sun"]
>>> arr.sort(reverse = True)
>>> arr
