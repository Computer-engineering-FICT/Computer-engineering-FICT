package kursach;

public class NodeModel {
        private boolean selected = false;
        private int weight;

        public NodeModel() {
        }

        public boolean isSelected() {
                return selected;
        }

        public void setSelected(boolean selected) {
                this.selected = selected;
        }

        public void setWeight(int weight) {
                this.weight = weight;
        }

        public int getWeight() {
               return weight;
        }
}
