#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = Lab1_1.exe Lab1_2.exe LAB1_3.exe Lab1_4.exe Manager.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Lab1_1.exe: LAB1_1\Lab1_1.dpr
  $(DCC)

Lab1_2.exe: LAB1_2\Lab1_2.dpr
  $(DCC)

LAB1_3.exe: Lab1_3\Lab1_3.DPR
  $(DCC)

Lab1_4.exe: Lab1_4\Lab1_4.dpr
  $(DCC)

Manager.exe: Manager\Manager.dpr
  $(DCC)


