Sub Macro1()
Attribute Macro1.VB_Description = "Macro recorded 05/29/00 by Slavikus"
Attribute Macro1.VB_ProcData.VB_Invoke_Func = "TemplateProject.NewMacros.Macro1"
'
' Macro1 Macro
' Macro recorded 05/29/00 by Slavikus
For i = 1 To ActiveDocument.Characters.Count
  If ActiveDocument.Characters(i) = "Q" Then ActiveDocument.Characters(i) = ChrW(9474)
  If ActiveDocument.Characters(i) = "I" Then ActiveDocument.Characters(i) = ChrW(9472)
  If ActiveDocument.Characters(i) = "O" Then ActiveDocument.Characters(i) = ChrW(9492)
  If ActiveDocument.Characters(i) = "U" Then ActiveDocument.Characters(i) = ChrW(9484)
  If ActiveDocument.Characters(i) = "T" Then ActiveDocument.Characters(i) = ChrW(9500)
  If ActiveDocument.Characters(i) = "F" Then ActiveDocument.Characters(i) = ChrW(9496)
  If ActiveDocument.Characters(i) = "D" Then ActiveDocument.Characters(i) = ChrW(9524)
  If ActiveDocument.Characters(i) = "E" Then ActiveDocument.Characters(i) = ChrW(9488)
  If ActiveDocument.Characters(i) = "W" Then ActiveDocument.Characters(i) = ChrW(9508)
  If ActiveDocument.Characters(i) = "R" Then ActiveDocument.Characters(i) = ChrW(9516)
  If ActiveDocument.Characters(i) = "Y" Then ActiveDocument.Characters(i) = ChrW(9532)

    
  Next i
End Sub
Sub Macro2()
Attribute Macro2.VB_Description = "Macro recorded 05/29/00 by Slavikus"
Attribute Macro2.VB_ProcData.VB_Invoke_Func = "TemplateProject.NewMacros.Macro2"
'
' Macro2 Macro
' Macro recorded 05/29/00 by Slavikus
'
    Selection.WholeStory
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "Q"
        .Replacement.Text = "A"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
End Sub
